<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App"
        tools:targetApi="31">
        <activity
            android:name=".AccountViewModel"
            android:exported="false"
            android:label="@string/title_activity_account_view_model"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".AccCreate"
            android:exported="false"
            android:label="@string/title_activity_acc_create"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".AccSelect"
            android:exported="false"
            android:label="@string/title_activity_acc_select"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".History"
            android:exported="false"
            android:label="@string/title_activity_history"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".Analysis"
            android:exported="false"
            android:label="@string/title_activity_analysis"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".Home"
            android:exported="false"
            android:label="@string/title_activity_home_page"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".LogIn"
            android:exported="false"
            android:label="@string/title_activity_log_in"
            android:theme="@style/Theme.App" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.App">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>